            <ab type="factoid" subtype="event" xml:id="factoid-15" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-15</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/ordination http://syriaca.org/keyword/bishops">
                        <desc>
                            Chalcedonians attempted to make <persName ref="http://syriaca.org/person/1771">Harfat</persName> the bishop of
                            <placeName ref="http://syriaca.org/place/2744">Arsamosata</placeName> but he refused.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">159</citedRange>
                </bibl>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">159</citedRange>
                </bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-16" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-16</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/laity http://syriaca.org/keyword/ordination http://syriaca.org/keyword/bishops">
                        <desc>
                            <persName ref="http://syriaca.org/person/3764">Anonymi 3764</persName>, though a layman, used his wealth to become bishop of
                            <placeName ref="http://syriaca.org/place/2744">Arsamosata</placeName>.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">160</citedRange>
                </bibl>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">160</citedRange>
                </bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-17" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-17</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/wealth http://syriaca.org/keyword/charity http://syriaca.org/keyword/monasticism">
                        <desc>
                            <persName ref="http://syriaca.org/person/1771">Harfat</persName> wanted to divide the family wealth and property in order to give it to the poor.
                            His kinsman <persName ref="http://syriaca.org/person/3764">Anonymi 3764</persName> refused. <persName ref="http://syriaca.org/person/1771">Harfat</persName>
                            took 5000 denarii cash and distributed it to the poor. Then he withdrew to a monastery.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">160</citedRange>
                </bibl>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">160</citedRange>
                </bibl>
            </ab>
