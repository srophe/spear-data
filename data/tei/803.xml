            <ab type="factoid" subtype="event" xml:id="factoid-12" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-12</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/inheritance">
                        <desc>
                            <persName ref="http://syriaca.org/person/1771">Harfat</persName> and his kinsman <persName ref="http://syriaca.org/person/3764">Anonymi 3764</persName>
                            inherited their family's considerable wealth.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">158</citedRange>
