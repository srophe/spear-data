                <idno type="URI">http://syriaca.org/spear/803-18</idno>
                <listPerson>
                    <person>
                        <persName ref="http://syriaca.org/person/1771"/>
                        <occupation ana="http://syriaca.org/keyword/monks">
                            <note type="desc">
                                <persName ref="http://syriaca.org/person/1771">Harfat</persName> 
                                was a monk.
                            </note>
                        </occupation>
                    </person>
                </listPerson>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">160</citedRange>
