            <ab type="factoid" subtype="event" xml:id="factoid-13" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-13</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/slavery http://syriaca.org/keyword/wealth">
                        <desc>
                            <persName ref="http://syriaca.org/person/1771">Harfat</persName> was distressed by the exploitation of 
                            <persName ref="http://syriaca.org/person/3765">Anonymi 3765</persName> and <persName ref="http://syriaca.org/person/3766">Anonymi 3766</persName>
                            by his kinsman <persName ref="http://syriaca.org/person/3764">Anonymi 3764</persName>. He does not approve of the great wealth of the estate
                            they inherited.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">159</citedRange>
                </bibl>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">159</citedRange>
                </bibl>
            </ab>
