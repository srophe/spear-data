                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">160</citedRange>
                </bibl>
            </ab>
            <ab type="factoid" subtype="event" xml:id="factoid-19" resp="#dschwartz">
                <idno type="URI">http://syriaca.org/spear/803-19</idno>
                <listEvent>
                    <event ana="http://syriaca.org/keyword/suffering http://syriaca.org/keyword/asceticism http://syriaca.org/keyword/sin http://syriaca.org/keyword/penitence">
                        <desc>
                            <persName ref="http://syriaca.org/person/1771">Harfat</persName>'s ascetic practice consisted of living in solitude on a remote 
                            mountain where he wept constantly in sorrow over his great sins.
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/bibl/1409"/>
                    <citedRange unit="p">161</citedRange>
                </bibl>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/work/803"/>
                    <citedRange unit="p">161</citedRange>
                </bibl>
            </ab>
            <ab type="factoid" subtype="gender" xml:id="factoid-21" resp="#dschwartz">
