            <ab type="factoid" subtype="event" xml:id="factoid-17" resp="#dschwartz">
                <idno type="URI">https://spear-prosop.org/3109-17</idno>
                <listEvent>
                    <event ana="http://syriaca.org/taxonomy/communion http://syriaca.org/taxonomy/anti-chalcedonian">
                        <desc>
                            Those who thought the <title>Henotikon</title> was insufficient without an 
                            anathema of Chalcedon refused to commune with those who thought is was sufficient. 
                        </desc>
                    </event>
                </listEvent>
                <bibl type="primary">
                    <ptr target="http://syriaca.org/cbss/8BIZTWME"/>
                    <citedRange unit="p">330</citedRange>
                    <citedRange unit="part">V.11</citedRange>
                </bibl>
                <note type="relatedEvent">
                    <listRelation>
